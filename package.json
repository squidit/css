{
  "name": "@squidit/css",
  "version": "1.1.7",
  "scripts": {
    "start": "gulp watch",
    "css": "sass src/scss/squid.scss:dist/css/squid.min.css --style compressed",
    "css:watch": "sass src/scss/squid.scss:dist/css/squid.min.css --style compressed --watch",
    "js": "uglifyjs src/js/**/*.js --compress --mangle --source-map --output dist/js/squid.min.js",
    "js:watch": "gulp watch-js",
    "dist": "npm-run-all --parallel css js",
    "lint": "npx eslint ./src/js/**/*.js"
  },
  "private": false,
  "files": [
    "dist/{css,js}/*.{css,js,map}",
    "dist/fonts/**/*.*",
    "src/js/**/*.{js,map}",
    "src/scss/**/*.scss"
  ],
  "dependencies": {},
  "devDependencies": {
    "eslint": "^7.26.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "gulp": "^4.0.2",
    "gulp-concat": "^2.6.1",
    "gulp-sass": "^4.1.0",
    "gulp-uglify": "^3.0.2",
    "husky": "^6.0.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.32.12",
    "uglify-js": "^3.13.5"
  },
  "keywords": [
    "css",
    "sass",
    "responsive",
    "front-end",
    "framework",
    "web"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/squidit/front-end.git"
  },
  "main": "dist/js/squid.min.js",
  "style": "dist/css/squid.min.css",
  "description": "Squid CSS Styleguide",
  "bugs": {
    "url": "https://github.com/squidit/front-end/issues"
  },
  "homepage": "https://squidit.github.io/css",
  "author": "Squid <developers@squidit.com.br>",
  "license": "MIT"
}
