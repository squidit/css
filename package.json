{
  "name": "front-end-squid",
  "version": "1.0.0",
  "scripts": {
    "start": "gulp watch",
    "css": "sass scss/squid.scss:dist/css/squid.min.css --style compressed",
    "css:watch": "sass scss/squid.scss:dist/css/squid.min.css --style compressed --watch",
    "js": "uglifyjs js/*.js --compress --mangle --source-map --output dist/js/squid.min.js",
    "js:watch": "gulp watch-js",
    "dist": "npm-run-all --parallel css js"
  },
  "private": false,
  "files": [
    "dist/{css,js}/*.{css,js,map}",
    "js/{src}/**/*.{js,map}",
    "scss/**/*.scss"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run dist"
    }
  },
  "dependencies": {},
  "devDependencies": {
    "autoprefixer": "^10.2.5",
    "eslint": "^7.26.0",
    "gulp": "^4.0.2",
    "gulp-concat": "^2.6.1",
    "gulp-sass": "^4.1.0",
    "gulp-uglify": "^3.0.2",
    "husky": "^6.0.0",
    "node-sass": "^6.0.0",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "sass": "^1.32.12",
    "uglify-js": "^3.13.5"
  },
  "keywords": [
    "css",
    "sass",
    "responsive",
    "front-end",
    "framework",
    "web"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/squidit/front-end.git"
  },
  "main": "dist/js/squid.js",
  "style": "dist/css/squid.css"
}
