{
  "name": "front-end-squid",
  "version": "1.0.0",
  "scripts": {
    "css": "sass scss/squid.scss:dist/css/squid.min.css --style compressed",
    "js": "uglifyjs js/*.js --output dist/js/squid.min.js --compress --mangle",
    "dist": "npm-run-all --parallel css js"
  },
  "private": false,
  "files": [
    "dist/{css,js}/*.{css,js,map}",
    "js/{src}/**/*.{js,map}",
    "scss/**/*.scss"
  ],
  "dependencies": {
  },
  "devDependencies": {
    "autoprefixer": "^10.2.5",
    "eslint": "^7.26.0",
    "node-sass": "^6.0.0",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "sass": "^1.32.12",
    "uglify-js": "^3.13.5"
  },
  "keywords": [
    "css",
    "sass",
    "mobile-first",
    "responsive",
    "front-end",
    "framework",
    "web"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/squidit/front-end.git"
  },
  "main": "dist/js/squid.js",
  "style": "dist/css/squid.css"
}
