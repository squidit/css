{
  "root": true,
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["standard"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "semi": ["off"],
    "no-unused-vars": ["off"],
    "no-undef": ["off"],
    "jsx-a11y/anchor-has-content": ["off"]
  }
}
